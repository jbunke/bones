path bones.examples;

import bones.stdlib.Math;

class DisjunctExample {
  void main(string[] args) {
    print("Please input a number: ");
    int ex = read();

    if (ex == disj { 13 | 11 | 10 | 14 }) {
      println("Matched in case 1");
    } else if (ex == disj { 2 * 6 | 0 }) {
      println("Matched in case 2");
    } else {
      println("No match found");
    }
  }
}